  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "servor": "^4.0.2",
    "tailwindcss": "^3.3.2"
  },
  "dependencies": {
    "heroicons": "^2.0.18"
  }
}
