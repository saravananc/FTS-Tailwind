{
  "name": "fts-tailwind",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tailwind:dev": "npx tailwindcss -i style.css -o ./src/style.css --watch",
    "tailwind:prod": "postcss style.css -o ./src/style.css",
    "server": "servor src Index.html 8080 --browse --reload",
    "dev": "run-p \"tailwind:dev\" \"server\"",
    "prod": "npm run tailwind:prod"
  },
